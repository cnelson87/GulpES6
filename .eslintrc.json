{
	"root": true,
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-unused-vars": ["error", { "args": "none" }],
		"curly": ["error", "all"],
		"semi": ["error", "always"]
	},
	"globals": {
		"module": true,
		"process": true,
		"require": true,
		"jQuery": true,
		"$": true,
		"_": true,
		"ga": true,
		"YT": true,
		"gsap": true,
		"Draggable": true,
		"ScrollTrigger": true,
		"moment": true,
		"noUiSlider": true,
		"Handlebars": true,
		"Application": true
	}
}
