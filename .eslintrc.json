{
	"root": true,
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-unused-vars": ["error", { "args": "none" }],
		"curly": ["error", "all"],
		"semi": ["error", "always"]
	},
	"globals": {
		"module": true,
		"process": true,
		"require": true,
		"Modernizr": true,
		"jQuery": true,
		"$": true,
		"_": true,
		"ga": true,
		"YT": true,
		"moment": true,
		"noUiSlider": true,
		"Handlebars": true,
		"ScrollMagic": true,
		"TweenMax": true,
		"TimelineMax": true,
		"Draggable": true,
		"Linear": true,
		"Back": true,
		"Bounce": true,
		"Circ": true,
		"Cubic": true,
		"Elastic": true,
		"Expo": true,
		"Power0": true,
		"Power1": true,
		"Power2": true,
		"Power3": true,
		"Power4": true,
		"Quad": true,
		"Quart": true,
		"Quint": true,
		"Sine": true,
		"Strong": true,
		"Application": true
	}
}
