<h1>{{primaryBlock.title}}</h1>
<div class="intro">
	{{#each primaryBlock.content}}
		<p>{{{this}}}</p>
	{{/each}}
</div>
{{#each level2Blocks}}
	<hr />
	<h2>{{title}}</h2>
	{{#each content}}
		<p>{{{this}}}</p>
	{{/each}}
	{{#each level3Blocks}}
		<h3>{{title}}</h3>
		{{#each content}}
			<p>{{{this}}}</p>
		{{/each}}
		{{#each level4Blocks}}
			<h4>{{title}}</h4>
			{{#each content}}
				<p>{{{this}}}</p>
			{{/each}}
		{{/each}}
	{{/each}}
{{/each}}
